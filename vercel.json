{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "web-build",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/_expo/*",
      "destination": "/_expo/:splat"
    },
    {
      "source": "/assets/*",
      "destination": "/assets/:splat"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}